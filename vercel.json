{
  "version": 2,
  "builds": [
    { "src": "server/app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/main/v2" },
    { "src": "/main/v2/(.*)", "dest": "server/app.js" },
    { "src": "/api/v2/(.*)", "dest": "server/app.js" },
    { "src": "/(.*)", "dest": "server/app.js" } 
  ]
}